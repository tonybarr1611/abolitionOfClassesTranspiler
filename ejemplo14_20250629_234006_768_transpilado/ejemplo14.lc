/protesta/ Verifica si un numero es multiplo de otro
imperialismo asamblea es_multiplo(numero, divisor){
    unidad resultado = 0
    abolir_clases((numero % divisor) == 0){
        rebelion resultado = 1
    } en_igualdad {
        rebelion resultado = 0
    }
    sentencia resultado
}

